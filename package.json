{
  "name": "vue-admin-template",
  "version": "0.1.0",
  "description": "vue管理端模板--带tag",
  "author": "lipfd@yonyou.com",
  "private": true,
  "homepage": "https://murphy573.github.io/vue-admin-template",
  "scripts": {
    "start": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "npm run lint && vue-cli-service build --mode production",
    "robot": "sh build/robot.sh",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "build:deploy": "npm run lint && vue-cli-service build --mode deploy",
    "deploy:github": "sh build/deploy.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "3.0.1",
    "axios": "1.2.1",
    "clipboard": "2.0.4",
    "core-js": "^3.8.3",
    "debug": "^4.3.4",
    "element-ui": "^2.13.2",
    "js-cookie": "2.2.0",
    "js-sha256": "^0.9.0",
    "lodash.clonedeep": "^4.5.0",
    "nprogress": "0.2.0",
    "screenfull": "4.2.0",
    "vue": "^2.6.14",
    "vue-i18n": "^8.17.2",
    "vue-lazyload": "1.3.4",
    "vue-router": "^3.5.1",
    "vue-tribute": "^1.0.7",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue": "^6.5.14",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-import": "^1.11.0",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "happypack": "^5.0.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "svg-sprite-loader": "^5.0.0",
    "svgo": "1.3.2",
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "^2.6.14"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
